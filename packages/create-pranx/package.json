{
  "name": "create-pranx",
  "version": "0.0.2",
  "description": "Starter cli for pranx",
  "type": "module",
  "scripts": {
    "build": "tsx ./scripts/build.ts",
    "build:prod": "bun run check && bun run build",
    "dev": "tsx watch --include \"./src/**/*\" --clear-screen=false ./scripts/build.ts",
    "test": "vitest run",
    "check": "tsc --noEmit -p ./tsconfig.json",
    "style": "bun run format && bun run lint",
    "format": "prettier --write --cache src scripts types",
    "lint": "biome lint . --fix --unsafe",
    "prepublishOnly": "bun run build:prod",
    "clean": "rm -rf dist node_modules",
    "release": "bun run build:prod && bun pm version patch && bun publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LiasCode/pranx.git"
  },
  "bugs": "https://github.com/LiasCode/pranx/issues",
  "homepage": "https://github.com/LiasCode/pranx",
  "bin": {
    "create-pranx": "dist/bin/index.js"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./types/index.d.ts"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "types",
    "README.md",
    "tsconfig.json",
    "LICENSE"
  ],
  "keywords": [
    "create-pranx",
    "pranx",
    "preact"
  ],
  "author": {
    "name": "LiasCode",
    "email": "liascode.dev@gmail.com",
    "url": "https://lias-code.pages.dev"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "license": "MIT",
  "dependencies": {
    "citty": "^0.1.6",
    "consola": "^3.4.2",
    "esbuild": "^0.25.9",
    "fs-extra": "^11.3.1",
    "giget": "2.0.0",
    "glob": "^11.0.3",
    "kleur": "^4.1.5",
    "ofetch": "^1.4.1",
    "pathe": "^2.0.3",
    "ufo": "^1.6.1"
  },
  "devDependencies": {
    "@biomejs/biome": "2.2.4",
    "@types/degit": "2.8.6",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^20.19.13",
    "prettier": "3.6.2",
    "prettier-plugin-organize-imports": "^4.2.0",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  }
}

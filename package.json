{
  "name": "pranx",
  "version": "0.0.57",
  "description": "The next of preact. A light-weight, Next.js-like fullstack metaframework",
  "type": "module",
  "scripts": {
    "build": "tsc -p ./tsconfig.json",
    "dev": "tsc --watch -p ./tsconfig.json",
    "test": "vitest run",
    "lint": "biome lint . --fix --unsafe",
    "check": "tsc --noEmit -p ./tsconfig.json",
    "format": "prettier --write --cache src",
    "prepublishOnly": "npm run build",
    "patch": "bun pm version patch && git push",
    "minor": "bun pm version minor && git push",
    "major": "bun pm version major && git push",
    "release": "changelogen --release --push && bun publish"
  },
  "repository": "https://github.com/LiasCode/pranx",
  "bugs": "https://github.com/LiasCode/pranx/issues",
  "homepage": "https://github.com/LiasCode/pranx",
  "bin": {
    "pranx": "dist/bin/index.js"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "module": "./dist/index.js",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "tsconfig.json",
    "LICENSE"
  ],
  "keywords": [
    "preact",
    "next",
    "ssr",
    "ssg",
    "isg",
    "csr",
    "file-based-router",
    "server",
    "esbuild",
    "metaframework",
    "fullstack"
  ],
  "author": {
    "name": "LiasCode",
    "email": "liascode.dev@gmail.com",
    "url": "https://lias-code.pages.dev"
  },
  "license": "MIT",
  "dependencies": {
    "@swc/html": "^1.13.3",
    "chokidar": "^4.0.3",
    "consola": "^3.4.2",
    "esbuild": "^0.25.8",
    "glob": "^11.0.3",
    "preact": "^10.27.0",
    "preact-render-to-string": "^6.5.13"
  },
  "devDependencies": {
    "@biomejs/biome": "1.8.3",
    "@types/chokidar": "^2.1.7",
    "@types/node": "^20.19.10",
    "changelogen": "^0.6.2",
    "prettier": "^3.6.2",
    "prettier-plugin-organize-imports": "^4.2.0",
    "tsx": "^4.20.3",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  },
  "trustedDependencies": [
    "@biomejs/biome"
  ]
}
